    <link rel="stylesheet" href="./css/d3style.css">
</head>
<body>
    <div class="wrapper">
        <nav id="sidebar">
            <ul class="list-unstyled components">
                <li> <a href="dashboard.html"> <i class="fas fa-home"></i> Dashboard</a> </li>
                <li> <a href="#studentSubmenu" data-toggle="collapse" aria-expanded="false" class="dropdown-toggle"><i class="fas fa-user-friends"></i> Students</a>
                    <ul class="collapse list-unstyled" id="studentSubmenu">
                        <!-- <li> <a href="register-student.html">Students</a> </li> -->
                        <li> <a href="all-students.html">All Students</a></li>
                    </ul>
                </li>
               
                <li> <a href="#userSubmenu" data-toggle="collapse" aria-expanded="false" class="dropdown-toggle"><i class="fas fa-users"></i> Manage Users</a>
                    <ul class="collapse list-unstyled" id="userSubmenu">
